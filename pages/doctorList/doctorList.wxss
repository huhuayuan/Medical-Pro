view{
  box-sizing: border-box;
}
.list-tmp {
  background: #fff;
}
.swipper-tab{
  border-bottom: 1px solid #e1e2e3;
}
.swiper-tab-list {
    display: inline-block;
    width: 30%;
    margin: 0 10%;
    font-size: 1.4em;
    padding: 0.6em 0;
    color: #101010;
    text-align: center;
}
.on {
    border-bottom: 3px solid #40c0e3;
}
.searchBox {
  background: #fff;
  padding: 0.5em;
  border-bottom: 1px solid #e1e2e3;
}
.searchBar {
  width: 94%;
  background: #f0f0f0;
  margin: 0 auto;
  color: #b2b2b2;
  border-radius: 6px;
  height: 2.6em;
  padding: 0.2em 0.8em;
}

.searchBar input {
    display: inline-block;
    width: 80%;
    font-size: 1.2em;
    padding-left: 0.2em;
}

.searchBar icon {
    display: inline-block;
    float: left;
    margin-top: 4px;
}
.doctorList {
  padding: 1.5em;
}
